<?xml version="1.0" encoding="UTF-8"?>
<excelImportConfigs>
    <config name="studentMsgImportConfiger">
        <classPath>com.adolesce.server.vo.excel.HeiMaStudentMsgImportVo</classPath>
        <!-- 实际起始行，第一行就是1 -->
        <startLine>2</startLine>
        <fields>
            <field>
                <name>name</name>
                <desc>姓名</desc>
                <columnIndex>0</columnIndex>
                <allowBlank>false</allowBlank>
            </field>
            <field>
                <name>sex</name>
                <desc>性别</desc>
                <columnIndex>1</columnIndex>
                <allowBlank>true</allowBlank>
                <regex>^[男,女]{1}$</regex>
                <regexMsg>性别应为{男/女}</regexMsg>
            </field>
            <field>
                <name>age</name>
                <desc>年龄</desc>
                <columnIndex>2</columnIndex>
                <allowBlank>true</allowBlank>
                <regex>^(?:[1-9][0-9]?|1[01][0-9]|120)$</regex>
                <regexMsg>年龄填写有误</regexMsg>
            </field>
            <field>
                <name>phone</name>
                <desc>联系方式</desc>
                <columnIndex>3</columnIndex>
                <allowBlank>true</allowBlank>
                <regex>^1[2|3|5|6|7|8|9]\d{9}$</regex>
                <regexMsg>手机号格式错误</regexMsg>
            </field>
            <field>
                <name>education</name>
                <desc>学历</desc>
                <columnIndex>4</columnIndex>
                <allowBlank>true</allowBlank>
            </field>
            <field>
                <name>school</name>
                <desc>毕业学校</desc>
                <columnIndex>5</columnIndex>
                <allowBlank>true</allowBlank>
            </field>
            <field>
                <name>specialty</name>
                <desc>专业</desc>
                <columnIndex>6</columnIndex>
                <allowBlank>true</allowBlank>
            </field>
            <field>
                <name>graduateTime</name>
                <desc>毕业时间</desc>
                <columnIndex>7</columnIndex>
                <allowBlank>true</allowBlank>
            </field>
            <field>
                <name>hopeSalary</name>
                <desc>期望薪资</desc>
                <columnIndex>8</columnIndex>
                <allowBlank>true</allowBlank>
            </field>
           <!-- <field>
                <name>salary</name>
                <desc>薪水</desc>
                <columnIndex>8</columnIndex>
                <allowBlank>true</allowBlank>
                <regex>^[0-9]+(\.[0-9]{1,2})?$</regex>
                <regexMsg>薪水一栏,请填写正整数或两位小数</regexMsg>
            </field>-->
            <field>
                <name>remark</name>
                <desc>备注</desc>
                <columnIndex>9</columnIndex>
                <allowBlank>true</allowBlank>
            </field>
        </fields>
    </config>
